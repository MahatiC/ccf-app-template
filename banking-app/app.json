        "openapi": {
          "responses": {
            "200": {
              "description": "Ok"
            }
          },
          "security": [],
          "parameters": [],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "value": {
                      "type": "number"
                    }
                  },
                  "type": "object"
                }
              }
            }
          }
