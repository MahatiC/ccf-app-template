{
  "endpoints": {
    "/deposit/{user_id}": {
      "post": {
        "js_module": "endpoints/banking.js",
        "js_function": "deposit",
        "forwarding_required": "always",
        "authn_policies": ["member_cert"],
        "mode": "readwrite",
        "openapi": {
          "responses": {
            "200": {
              "description": "Ok"
            }
          },
          "security": [],
          "parameters": [],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "value": {
                      "type": "number"
                    }
                  },
                  "type": "object"
                }
              }
            }
          }
        }
      }
    },
    "/balance": {
      "get": {
        "js_module": "endpoints/banking.js",
